<mvc:View controllerName="OrderSystem.ordersystem.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.uxap"  xmlns:layout="sap.ui.layout" 
	xmlns:m="sap.m">
    <ObjectPageLayout id="ObjectPageLayout" enableLazyLoading="true" upperCaseAnchorBar="false">
    <headerTitle>
			<ObjectPageDynamicHeaderTitle id="_IDGenObjectPageDynamicHeaderTitle1">
				<heading>
					<m:Title id="_IDGenTitle1" text="Order Managment System"/>
				</heading>

         
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
   
        <sections>
			<ObjectPageSection id="ObjectPageSectionHeader" titleUppercase="false" title="Header">
				<subSections>
					<ObjectPageSubSection id="ObjectPageSubSectionHeader" titleUppercase="false">
						<blocks>
								<mvc:XMLView id="XMLView1Header" viewName="OrderSystem.ordersystem.view.Header"></mvc:XMLView>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>


            	<ObjectPageSection id="ObjectPageSectionItem" titleUppercase="false" title="Items">
				<subSections>
					<ObjectPageSubSection id="ObjectPageSubSectionItem" titleUppercase="false">
						<blocks>
								<mvc:XMLView id="XMLViewItem" viewName="OrderSystem.ordersystem.view.Item"></mvc:XMLView>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>


				<ObjectPageSection id="ObjectPageSectionPromoVistex" titleUppercase="false" title="Promo Vistex">
				<subSections>
					<ObjectPageSubSection id="ObjectPageSubSectionPromoVistex" titleUppercase="false">
						<blocks>
								<mvc:XMLView id="XMLViewPromoVistex" viewName="OrderSystem.ordersystem.view.PromoVistex"></mvc:XMLView>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
            

			<ObjectPageSection id="ObjectPageSectionHeaderPricing" titleUppercase="false" title="Header Pricing">
				<subSections>
					<ObjectPageSubSection id="ObjectPageSubSectionHeaderPricing" titleUppercase="false">
						<blocks>
								<mvc:XMLView id="XMLViewHeaderPricing" viewName="OrderSystem.ordersystem.view.HeaderPricing"></mvc:XMLView>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>


			<ObjectPageSection id="ObjectPageSectionPartners" titleUppercase="false" title="Partners">
				<subSections>
					<ObjectPageSubSection id="ObjectPageSubSectionPartners" titleUppercase="false">
						<blocks>
								<mvc:XMLView id="XMLViewPartners" viewName="OrderSystem.ordersystem.view.Partners"></mvc:XMLView>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>


				<ObjectPageSection id="ObjectPageSectionNotes" titleUppercase="false" title="Notes">
				<subSections>
					<ObjectPageSubSection id="ObjectPageSubSectionNotes" titleUppercase="false">
						<blocks>
								<mvc:XMLView id="XMLViewNotes" viewName="OrderSystem.ordersystem.view.Notes"></mvc:XMLView>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>


		</sections>
    </ObjectPageLayout>
</mvc:View>

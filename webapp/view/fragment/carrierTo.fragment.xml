<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" xmlns:c="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:ui="sap.ui" 
   xmlns:f="sap.ui.layout.form">
  
     	<TableSelectDialog id="_IDGenTableSelectDialog1.1"
		noDataText="No Data"
		title="Freight Forwarder Selection"
		search=".handleSearch"
        confirm=".handleValueHelpCloseCarrier"
		cancel=".handleValueHelpCloseCarrier"
		items="{oOrderData>/shipToData}">
		<ColumnListItem id="_IDGenColumnListItem1.1" selected="{selected}">
			<cells>
					<ObjectIdentifier id="_IDGenObjectIdentifier12" title="{oOrderData>partnerCode}" text="{oOrderData>partnerCode}" />
				<Text id="_IDGenText1.1" text="{oOrderData>partner}" />
				<Text id="_IDGenText2.1" text="{oOrderData>prov}" />
				<Text id="_IDGenText3.1" text="{oOrderData>city}" />
				<Text id="_IDGenText4.1" text="{oOrderData>village}" />
				<Text id="_IDGenText5.1" text="{oOrderData>address}" />
				<Text id="_IDGenText6.1" text="{oOrderData>postalCode}" />
				
			</cells>
		</ColumnListItem>
		<columns>
			<Column id="_IDGenColumn12" width="12em">
				<header>
					<Text id="_IDGenText7.1" text="{i18n>partnerCode}" />
				</header>
			</Column>
			<Column id="_IDGenColumn21" minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text id="_IDGenText8.1" text="{i18n>partner}" />
				</header>
			</Column>
			<Column id="_IDGenColumn3.1"
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="End">
				<header>
					<Text id="_IDGenText9.1" text="{i18n>prov}" />
				</header>
			</Column>
			<Column id="_IDGenColumn4.1"
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<header>
					<Text id="_IDGenText10.1" text="{i18n>city}" />
				</header>
			</Column>
			<Column id="_IDGenColumn5.1" hAlign="End">
				<header>
					<Text id="_IDGenText1.12" text="{i18n>village}" />
				</header>
			</Column>
            <Column id="_IDGenColumn6.1" hAlign="End">
				<header>
					<Text id="_IDGenText12.1" text="{i18n>address}" />
				</header>
			</Column>
            <Column id="_IDGenColumn7.1" hAlign="End">
				<header>
					<Text id="_IDGenText13.1" text="{i18n>postalCode}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
       
   
  
         

</core:FragmentDefinition>

<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" xmlns:c="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:ui="sap.ui" 
   xmlns:f="sap.ui.layout.form">
  
     	<TableSelectDialog id="_IDGenTableSelectDialog12"
		noDataText="No Data"
		title="Goods Recipient Selection"
		search=".handleSearch"
        confirm=".handleValueHelpClose"
		cancel=".handleValueHelpClose"
		items="{oOrderData>/ProductData}">
		<ColumnListItem id="_IDGenColumnListItem12" selected="{selected}">
			<cells>
					<ObjectIdentifier id="_IDGenObjectIdentifier1" title="{oOrderData>productcode}"  />
				<Text id="_IDGenText12" text="{oOrderData>Product}" />
				<Text id="_IDGenText22" text="{oOrderData>materialGroup}" />
				<Text id="_IDGenText32" text="{oOrderData>gerlevel1}" />
				<Text id="_IDGenText42" text="{oOrderData>gerlevel2}" />
				<Text id="_IDGenText52" text="{oOrderData>gerlevel3}" />
				<Text id="_IDGenText62" text="{oOrderData>gerlevel4}" />
				<Text id="_IDGenText63" text="{oOrderData>attribute1}" />
				<Text id="_IDGenText64" text="{oOrderData>attribute2}" />
				<Text id="_IDGenText65" text="{oOrderData>attribute3}" />
				<Text id="_IDGenText66" text="{oOrderData>attribute4}" />
				<Text id="_IDGenText67" text="{oOrderData>attribute5}" />
				<Text id="_IDGenText68" text="{oOrderData>UoMSale}" />
				<Text id="_IDGenText69" text="{oOrderData>priceList}" />
				<Text id="_IDGenText70" text="{oOrderData>discount}" />
				
			</cells>
		</ColumnListItem>
		<columns>
			<Column id="_IDGenColumn123" width="12em">
				<header>
					<Text id="_IDGenText72" text="{i18n>partnerCode}" />
				</header>
			</Column>
			<Column id="_IDGenColumn22" minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text id="_IDGenText82" text="{i18n>product}" />
				</header>
			</Column>
			<Column id="_IDGenColumn32"
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="End">
				<header>
					<Text id="_IDGenText92" text="{i18n>materialGroup}" />
				</header>
			</Column>
			<Column id="_IDGenColumn42"
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<header>
					<Text id="_IDGenText102" text="{i18n>gerlevel1}" />
				</header>
			</Column>
			<Column id="_IDGenColumn52" hAlign="End">
				<header>
					<Text id="_IDGenText112" text="{i18n>gerlevel2}" />
				</header>
			</Column>
            <Column id="_IDGenColumn62" hAlign="End">
				<header>
					<Text id="_IDGenText122" text="{i18n>gerlevel3}" />
				</header>
			</Column>
            <Column id="_IDGenColumn72" hAlign="End">
				<header>
					<Text id="_IDGenText133" text="{i18n>gerlevel4}" />
				</header>
			</Column>
			<Column id="_IDGenColumn73" hAlign="End">
				<header>
					<Text id="_IDGenText134" text="{i18n>attribute1}" />
				</header>
			</Column>
			<Column id="_IDGenColumn74" hAlign="End">
				<header>
					<Text id="_IDGenText135" text="{i18n>attribute2}" />
				</header>
			</Column>
			<Column id="_IDGenColumn75" hAlign="End">
				<header>
					<Text id="_IDGenText136" text="{i18n>attribute3}" />
				</header>
			</Column>
			<Column id="_IDGenColumn76" hAlign="End">
				<header>
					<Text id="_IDGenText137" text="{i18n>attribute4}" />
				</header>
			</Column>
			<Column id="_IDGenColumn777" hAlign="End">
				<header>
					<Text id="_IDGenText138" text="{i18n>attribute5}" />
				</header>
			</Column>
			<Column id="_IDGenColumn78" hAlign="End">
				<header>
					<Text id="_IDGenText139" text="{i18n>UoMSale}" />
				</header>
			</Column>
			<Column id="_IDGenColumn79" hAlign="End">
				<header>
					<Text id="_IDGenText140" text="{i18n>priceList}" />
				</header>
			</Column>
			<Column id="_IDGenColumn80" hAlign="End">
				<header>
					<Text id="_IDGenText141" text="{i18n>discount}" />
				</header>
			</Column>
			

		</columns>
	</TableSelectDialog>
       
</core:FragmentDefinition>

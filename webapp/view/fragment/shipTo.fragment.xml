<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" xmlns:c="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:ui="sap.ui" 
   xmlns:f="sap.ui.layout.form">
  
     	<TableSelectDialog id="_IDGenTableSelectDialog1"
		noDataText="No Data"
		title="Select Address"
		
		items="{oOrderData>/shipToData}">
		<ColumnListItem id="_IDGenColumnListItem1" >
			<cells>
					<ObjectIdentifier id="_IDGenObjectIdentifier1" title="{oOrderData>partnerCode}" text="{oOrderData>partnerCode}" />
				<Text id="_IDGenText1" text="{oOrderData>partner}" />
				<Text id="_IDGenText2" text="{oOrderData>prov}" />
				<Text id="_IDGenText3" text="{oOrderData>city}" />
				<Text id="_IDGenText4" text="{oOrderData>village}" />
				<Text id="_IDGenText5" text="{oOrderData>address}" />
				<Text id="_IDGenText6" text="{oOrderData>postalCode}" />
				
			</cells>
		</ColumnListItem>
		<columns>
			<Column id="_IDGenColumn1" width="12em">
				<header>
					<Text id="_IDGenText7" text="{i18n>partnerCode}" />
				</header>
			</Column>
			<Column id="_IDGenColumn2" minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text id="_IDGenText8" text="{i18n>partner}" />
				</header>
			</Column>
			<Column id="_IDGenColumn3"
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="End">
				<header>
					<Text id="_IDGenText9" text="{i18n>prov}" />
				</header>
			</Column>
			<Column id="_IDGenColumn4"
				minScreenWidth="Desktop"
				demandPopin="true"
				hAlign="Center">
				<header>
					<Text id="_IDGenText10" text="{i18n>city}" />
				</header>
			</Column>
			<Column id="_IDGenColumn5" hAlign="End">
				<header>
					<Text id="_IDGenText11" text="{i18n>village}" />
				</header>
			</Column>
            <Column id="_IDGenColumn6" hAlign="End">
				<header>
					<Text id="_IDGenText12" text="{i18n>address}" />
				</header>
			</Column>
            <Column id="_IDGenColumn7" hAlign="End">
				<header>
					<Text id="_IDGenText13" text="{i18n>postalCode}" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
       
   
  
         

</core:FragmentDefinition>
